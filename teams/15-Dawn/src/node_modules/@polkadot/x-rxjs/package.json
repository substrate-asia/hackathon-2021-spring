{
  "name": "@polkadot/x-rxjs",
  "version": "6.0.5",
  "type": "module",
  "description": "A pass-through interface for RxJs (allowing node + mjs usage)",
  "main": "index.js",
  "sideEffects": false,
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "maintainers": [],
  "contributors": [],
  "license": "Apache-2.0",
  "repository": "github:polkadot-js/common",
  "bugs": "https://github.com/polkadot-js/common/issues",
  "homepage": "https://github.com/polkadot-js/common/tree/master/packages/x-rxjs#readme",
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "rxjs": "^6.6.6"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./cjs": "./cjs/index.js",
    "./cjs/index.d.ts": "./cjs/index.d.ts",
    "./cjs/operators": "./cjs/operators.js",
    "./cjs/operators.d.ts": "./cjs/operators.d.ts",
    "./cjs/package.json": "./cjs/package.json",
    "./index.d.ts": "./index.d.ts",
    "./operators": {
      "require": "./operators.cjs",
      "default": "./operators.js"
    },
    "./operators.d.ts": "./operators.d.ts",
    "./package.json": "./package.json",
    "./packageInfo": {
      "require": "./packageInfo.cjs",
      "default": "./packageInfo.js"
    },
    "./packageInfo.d.ts": "./packageInfo.d.ts",
    "./README.md": "./README.md"
  }
}